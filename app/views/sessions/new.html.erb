<% set_title("Log In") %>
<menu>
  <%= link_to "Home", root_path %>
</menu>

<p class="note_subject">Log In</p>

<% content_for :page do %>

  <%= form_tag sessions_path do %>
    <p>
      <%= label_tag :email %><br />
      <%= text_field_tag :email, params[:email] %>
    </p>

    <p>
      <%= label_tag :password %><br />
      <%= password_field_tag :password %>
    </p>

    <%= submit_tag "Log In" %>
  <% end %>
<% end %>

