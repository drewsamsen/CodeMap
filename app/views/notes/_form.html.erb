<%= form_for(@note) do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :subject %><br/>
    <%= f.text_field :subject %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description, :size => "24x6" %>
  </p>
  <p>
    <%= f.label :technology_name %><br />
    <%= f.collection_select :technology_name, Technology.find(:all), :name, :name %>
  </p>

  <%= f.submit %>
<% end %>